# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                           controllers.HomeController.index
POST    /                           controllers.HomeController.authenticate
# An example controller showing how to use dependency injection
GET     /count                      controllers.CountController.count
# An example controller showing how to write asynchronous code
GET     /message                    controllers.AsyncController.message

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

#In this method we can add person to the database
GET    /addManager                  controllers.HomeController.addManager

GET    /init                        controllers.HomeController.addSemesterInfoFile
POST   /init                        controllers.HomeController.init
GET    /home                        controllers.HomeController.home
#In this method we can add person to the database

#GET    /init                        controllers.HomeController.init


GET /test                           controllers.HomeController.test
GET /duplicate/:id                      controllers.HomeController.duplicate(id:Integer)
POST /duplicate/:id                     controllers.HomeController.submitDuplicate(id:Integer)
GET /course_profile/:id             controllers.HomeController.courseProfile(id:Integer)